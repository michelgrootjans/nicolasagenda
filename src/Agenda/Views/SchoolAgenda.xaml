<Window x:Class="Agenda.Views.SchoolAgenda"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="SchoolAgenda" Height="400" Width="800"
        Closing="SaveView">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Name="navigation" />
        <StackPanel Grid.Column="1" Name="content" />
    </Grid>
</Window>
